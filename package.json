{
  "name": "facepay",
  "version": "1.0.0",
  "description": "FacePay - Biometric Payment Platform with Facial Recognition Authentication",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:api": "jest __tests__/api",
    "test:components": "jest __tests__/components",
    "test:services": "jest __tests__/services",
    "test:critical": "jest __tests__/services __tests__/api/auth __tests__/api/payments __tests__/components/BiometricWithFallback.test.tsx",
    "test:coverage:critical": "jest --coverage __tests__/services __tests__/api/auth __tests__/api/payments __tests__/components/BiometricWithFallback.test.tsx",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:init": "node scripts/init-db.js",
    "db:fresh": "node scripts/init-db.js --fresh",
    "test:users": "node scripts/generate-test-users.js",
    "test:users:fresh": "node scripts/generate-test-users.js --fresh",
    "setup": "./scripts/quick-start.sh",
    "deploy": "npm run build && npm run test:critical && echo 'ðŸš€ Ready for deployment! Push to main branch for auto-deploy.'",
    "deploy:vercel": "vercel --prod",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@metamask/sdk": "^0.28.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^15.5.2",
