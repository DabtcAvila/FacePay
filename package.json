    "html2canvas": "^1.4.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.427.0",
    "mercadopago": "^2.8.0",
    "mixpanel-browser": "^2.70.0",
    "next": "^14.2.0",
