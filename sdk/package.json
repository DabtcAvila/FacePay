{
  "name": "@facepay/sdk",
  "version": "1.0.0-production",
  "description": "Production-ready biometric payment authentication SDK with Stripe integration - Complete payment solution in one line of code!",
  "main": "facepay-sdk.js",
  "types": "facepay-sdk.d.ts",
  "files": [
    "facepay-sdk.js",
    "facepay-sdk.d.ts", 
    "README-production.md",
    "examples/"
  ],
  "scripts": {
    "test": "node test.js",
    "demo": "python -m http.server 8000",
    "serve": "npx serve .",
    "size": "du -h facepay-sdk.js",
    "build": "npm run minify",
    "minify": "terser facepay-sdk.js -o facepay-sdk.min.js --compress --mangle",
    "analyze": "bundlesize"
  },
  "keywords": [
    "biometric",
    "payment",
    "authentication",
    "stripe",
    "webauthn",
    "faceid",
    "touchid",
    "fingerprint",
    "security",
    "sdk",
    "javascript",
    "typescript",
    "face-recognition",
    "biometric-auth",
    "passwordless",
    "2fa",
    "mfa",
    "payments",
    "checkout",
    "ecommerce",
    "fintech"
  ],
  "author": "FacePay Team <team@facepay.com>",
  "license": "MIT",
  "homepage": "https://github.com/facepay/sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/facepay/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/facepay/sdk/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": "facepay-sdk.js",
  "module": "facepay-sdk.js",
  "unpkg": "facepay-sdk.js",
  "jsdelivr": "facepay-sdk.js",
  "cdn": "facepay-sdk.js",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/facepay"
  },
  "config": {
    "bundle_size_limit": "15KB",
    "current_size": "14.8KB"
  },
  "bundlesize": [
    {
      "path": "./facepay-sdk.js",
      "maxSize": "50KB"
    }
  ],
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {},
  "optionalDependencies": {},
  "bundledDependencies": [],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/facepay"
  }
}