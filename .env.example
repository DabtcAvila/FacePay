# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env files with real credentials

# Database
DATABASE_URL="your-database-url-here"
DIRECT_URL="your-direct-database-url-here"

# Authentication Secrets (Generate new ones with: openssl rand -base64 32)
NEXTAUTH_SECRET="your-secure-nextauth-secret-here"
JWT_SECRET="your-secure-jwt-secret-here"
JWT_REFRESH_SECRET="your-secure-jwt-refresh-secret-here"

# App Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"

# Supabase
NEXT_PUBLIC_SUPABASE_URL="your-supabase-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"

# WebAuthn
NEXT_PUBLIC_WEBAUTHN_RP_NAME="FacePay"
NEXT_PUBLIC_WEBAUTHN_RP_ID="localhost"
WEBAUTHN_ORIGIN="http://localhost:3000"

# Features
NEXT_PUBLIC_ENABLE_MONITORING="false"
NEXT_PUBLIC_ENABLE_ANALYTICS="false"

# Payment Providers (Optional)
# STRIPE_SECRET_KEY=""
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=""
# MERCADOPAGO_ACCESS_TOKEN=""
# NEXT_PUBLIC_MERCADOPAGO_PUBLIC_KEY=""